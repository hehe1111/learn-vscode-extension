{
  "name": "learn-vscode-extension",
  "displayName": "Learn VSCode Extension",
  "repository": "git@github.com:hehe1111/learn-vscode-extension.git",
  "description": "A VSCode extension for learning purposes with command palette and context menu support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0",
    "node": ">=22"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "learn-vscode-extension.showInputCommand",
        "title": "learn-vscode"
      },
      {
        "command": "learn-vscode-extension.showInputContextMenu",
        "title": "learn-vscode"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "learn-vscode-extension.showInputContextMenu",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm lint",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
